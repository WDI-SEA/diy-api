<h2> Saved Pokemon Cards </h2>
<div style="display: flex; flex-wrap: wrap; text-align: center;">
    <% pokemoncards.forEach(pokemon => { %>
        <div style="display: inline-block;">
            <h3><%=pokemon.name%></h3>
            <img src="<%=pokemon.img_url%>"/> 
            <p><%=pokemon.rarity%></p>
            <a href="/pokemoncards/<%=pokemon.id%>"></a>
        </div>
        <hr/>
    <% }) %>
    </div>